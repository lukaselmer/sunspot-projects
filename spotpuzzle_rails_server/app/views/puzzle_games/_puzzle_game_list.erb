<table>
  <tr>
    <th>Anzahl Aktionen</th>
    <th>Spielzyklen</th>
    <th>Spieldauer</th>
    <th>Gespielt am</th>
  </tr>

  <% puzzle_games.each do |puzzle_game| %>
    <tr class="<%= cycle("even", "odd") -%>">
      <td><%= puzzle_game.swap_times %></td>
      <td><%= puzzle_game.cycle_times %></td>
      <td><%= puzzle_game.game_time_in_words %></td>
      <td><%= l puzzle_game.created_at %></td>
    </tr>
  <% end %>
</table>
